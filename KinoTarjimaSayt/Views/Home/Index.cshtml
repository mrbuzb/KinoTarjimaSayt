@model List<KinoTarjimaMVC.Models.Movie>

@{
    ViewData["Title"] = "Kinolar ro'yxati";
}

<h2>Kinolar ro'yxati</h2>

@foreach (var movie in Model)
{
    <div style="margin-bottom: 20px;">
        <a href="@Url.Action("Watch", "Home", new { title = movie.Title })" style="font-size: 20px;">
            🎬 @movie.Title
        </a>
    </div>
}
